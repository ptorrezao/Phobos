<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>@ViewBag.PageTitle</title>
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    @Styles.Render("~/Content/bootstrap")
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css">
    <link rel="stylesheet" href="https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css">
    @Styles.Render("~/Content/AdminLTE")
    <!--[if lt IE 9]>
        @Scripts.Render("~/Content/respond")
        @Scripts.Render("~/Content/html5shiv")
    <![endif]-->
</head>
<body class="hold-transition skin-blue sidebar-mini">
    @StackExchange.Profiling.MiniProfiler.RenderIncludes()
    <div class="wrapper">
        @Html.Partial("_MainHeader")
        @Html.Partial("_MainSidebar")
        <div class="content-wrapper">
            @RenderSection("_ContentHeader", required: false)
            <section class="content">
                @RenderBody()
                @ViewBag.PageTitle
            </section>
        </div>
        @Html.Partial("_MainFooter")
        @Html.Partial("_ControlSidebar")
        <div class="control-sidebar-bg"></div>
    </div>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/AdminLTE")
    @Scripts.Render("~/bundles/timeago")
    @RenderSection("scripts", required: false)
</body>
</html>
